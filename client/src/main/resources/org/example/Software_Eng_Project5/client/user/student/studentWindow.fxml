<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="310.0" prefWidth="575.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Software_Eng_Project5.client.user.student.GUI.mainStudentController">
   <children>
      <Button layoutX="80.0" layoutY="125.0" mnemonicParsing="false" onAction="#executeExam" text="Execute Exam">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="80.0" layoutY="176.0" mnemonicParsing="false" onAction="#seeExams" prefHeight="78.0" prefWidth="132.0" text="See my&#10; Exams" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <TextField fx:id="executionCodeTF" layoutX="222.0" layoutY="125.0" prefHeight="39.0" prefWidth="164.0" promptText="Execution code">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="idTF" layoutX="397.0" layoutY="125.0" prefHeight="39.0" prefWidth="164.0" promptText="ID">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label fx:id="userNameL" layoutX="32.0" layoutY="14.0" prefHeight="39.0" prefWidth="112.0" text="user Name">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="errorLabel" layoutX="212.0" layoutY="84.0" prefHeight="25.0" prefWidth="172.0" text="This exam was already executed" textFill="#e10000" />
   </children>
</AnchorPane>
