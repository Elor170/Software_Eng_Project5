<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="250.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Software_Eng_Project5.client.PrimaryController">
   <children>
      <Label alignment="CENTER" prefHeight="20.0" prefWidth="76.0" text="Welcome">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Label>
      <Label alignment="CENTER" prefHeight="17.0" prefWidth="73.0" text="User Name:">
         <VBox.margin>
            <Insets bottom="5.0" right="180.0" />
         </VBox.margin></Label>
      <TextField fx:id="userNameTF" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="250.0" promptText="Please enter teacher name">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin></TextField>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="65.0" text="Password:">
         <VBox.margin>
            <Insets bottom="5.0" right="180.0" />
         </VBox.margin></Label>
      <TextField fx:id="passwordTF" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="250.0" promptText="Please enter password">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin></TextField>
      <Button fx:id="logInB" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#logInB" prefHeight="25.0" prefWidth="70.0" text="Log in" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets left="20.0" right="20.0" />
   </padding>
</VBox>
